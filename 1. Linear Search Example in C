#include <stdio.h>
int main() {
    int arr[] = {7, 9, 12, 6, 5, 4};
    int n = 6, target = 8, found = 0;
    for(int i = 0; i < n; i++) {
        if(arr[i] == target) {
            found = 1;
            printf("Element %d found at index %d\n", target, i);
            break;
        }
    }
    if(!found)
        printf("Element %d not found in array\n", target);
    return 0;
}
